{
    "name": "licenser",
    "displayName": "licenser",
    "description": "inserts license header in source files and license file.",
    "version": "0.0.1",
    "publisher": "ymotongpoo",
    "engines": {
        "vscode": "^0.10.10"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.insertLicenseHeader",
        "onCommand:extension.createLicenseFile"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "title": "Licenser configurations",
            "properties": {
                "licenser.License": {
                    "type": "string",
                    "default": "AL2",
                    "description": "License setting for workspace. Available licenses are 'AL2', 'GPLv2', 'GPLv3', 'LGPLv3', 'MIT', 'BSD'"
                },
                "licenser.Author": {
                    "type": "string",
                    "default": "Yoshi Yamaguchi",
                    "description": "Author name of the project."
                }
            }
        },
        "commands": [
            {
                "command": "extension.insertLicenseHeader",
                "title": "Insert license header"
            },
            {
                "command": "extension.createLicenseFile",
                "title": "Create license file"
            }
        ],
        "keybindings": [
            {
                "command": "extension.createLicenseFile",
                "key": "shift+cmd+ctrl+l",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}